{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "antoniotre86/multi-client",
			"hostname": "client", 
			"essential": false
		},
		{
			"name": "server",
			"image": "antoniotre86/multi-server",
			"hostname": "api", 
			"essential": false
		},
		{
			"name": "worker",
			"image": "antoniotre86/multi-worker",
			"hostname": "worker", 
			"essential": false
		},
		{
			"name": "nginx",
			"image": "antoniotre86/multi-nginx",
			"essential": true,
			"portMappings" :[
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["client", "server"]
		}
	]
}